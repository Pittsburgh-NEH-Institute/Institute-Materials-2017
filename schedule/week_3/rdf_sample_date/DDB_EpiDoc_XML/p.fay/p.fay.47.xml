    <lb n="4"/><expan>Πετεσοῦχο<ex>ς</ex></expan> Ὀρσενούφεως 

    <lb n="5"/><expan>ὑ<ex>πὲρ</ex></expan> <app type="editorial"><lem resp="BL 8.122"><expan>παραζυτοπ<ex>οιίας</ex></expan></lem><rdg><abbr>παραζυτ</abbr></rdg></app> κατʼ <expan><unclear>ἄν</unclear>δ<ex>ρα</ex></expan> <app type="editorial"><lem resp="BL 8.122"><abbr>ζυτοπ</abbr></lem><rdg><abbr>ζυ<unclear>τ</unclear></abbr></rdg></app> 

    <lb n="6"/><expan>Εὐημ<ex>ερείας</ex></expan> τοῦ αὐτοῦ <expan><ex>ἔτους</ex></expan> 

    <lb n="7"/>ἐπὶ <expan>λόγο<ex>υ</ex></expan> <expan><ex>δραχμὰς</ex></expan> <choice><reg>τέσσαρας</reg><orig>τέσσαρ<unclear>ε</unclear>ς</orig></choice>, <expan><ex>γίνονται</ex></expan> <expan><ex>δραχμαὶ</ex></expan> <num value="4">δ</num>, 

    <lb n="8"/>καὶ τῇ <num value="15">ιε</num> <expan>ὁμοί<ex>ως</ex></expan> ἐπὶ <expan>λ<ex>όγου</ex></expan> <expan><ex>δραχμὰς</ex></expan> <expan>τέσσαρ<ex>ας</ex></expan>, 

    <lb n="9"/><expan><ex>γίνονται</ex></expan> <expan><ex>δραχμαὶ</ex></expan> <num value="4">δ</num>. 

    <lb n="10"/>ἔτους <num value="8"><unclear>η</unclear></num> <supplied reason="lost">Νέ</supplied>ρων<supplied reason="lost">ο</supplied><unclear>ς</unclear> Κ<supplied reason="lost">λ</supplied>αυδ<supplied reason="lost">ίου</supplied> 

