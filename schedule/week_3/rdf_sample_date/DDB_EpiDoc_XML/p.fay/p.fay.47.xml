    <lb n="12"/><expan>δι<ex>έγραψε</ex></expan> Πετεσοῦχος <expan>Ὀρσενο<ex>ύφεως</ex></expan> ἀπὸ <expan><unclear>τιμῆ</unclear><ex>ς</ex></expan> 

    <lb n="13"/>ζύτου ἐπὶ <expan>λόγο<ex>υ</ex></expan> <expan><ex>δραχμὰς</ex></expan> <choice><reg>ὀκτώ</reg><orig>ὀκτώι</orig></choice>, <expan><ex>γίνονται</ex></expan> <expan><ex>δραχμαὶ</ex></expan> <num value="8">η</num>, 

